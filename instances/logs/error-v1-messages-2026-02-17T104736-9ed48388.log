n    94→    if (inRouting && /^\\s*\\[[a-zA-Z]/.test(line) && !/^\\s*\\[routing\\]/.test(line)) break;\n    95→    if (inRouting) {\n    96→      const match = line.match(/^\\s*\"([^\"]+)\"\\s*=\\s*\\[/);\n    97→      if (match) models.push(match[1]);\n    98→    }\n    99→  }\n   100→  return models;\n   101→}\n   102→\n   103→// Parse main_port from [global] section\n   104→function parseMainPort() {\n   105→  const content = readTomlContent();\n   106→  if (!content) return null;\n   107→  const match = content.match(/^main_port\\s*=\\s*(\\d+)/m);\n   108→  return match ? parseInt(match[1]) : null;\n   109→}\n   110→\n   111→// Parse lb_auto_upgrade_map from TOML: { \"g3f.auto\": \"opus4.6\", ... }\n   112→function parseAutoUpgradeMap() {\n   113→  const content = readTomlContent();\n   114→  if (!content) return {};\n   115→  const map = {};\n   116→  let inSection = false;\n   117→  for (const line of content.split('\\n')) {\n   118→    if (/^\\s*\\[global\\.lb_auto_upgrade_map\\]\\s*$/.test(line)) { inSection = true; continue; }\n   119→    if (inSection && /^\\s*\\[/.test(line)) break;\n   120→    if (inSection) {\n   121→      const m = line.match(/^\\s*\"([^\"]+)\"\\s*=\\s*\"([^\"]+)\"/);\n   122→      if (m) map[m[1]] = m[2];\n   123→    }\n   124→  }\n   125→  return map;\n   126→}\n   127→\n   128→// --- Scenario Presets (derived from cld script) ---\n   129→// These map scenario names to model role assignments.\n   130→// The actual model IDs come from TOML routing, so the presets just reference\n   131→// them by their routing key names. If a routing key doesn't exist, we let the\n   132→// proxy handle the fallback.\n   133→\n   134→const SCENARIO_PRESETS = {\n   135→  auto:        { main: 'auto',    fast: 'auto', opus: 'opus4.6', desc: '智能路由，按复杂度自动选模型' },\n   136→  performance: { main: 'opus4.6', fast: 'g3f',  opus: 'opus4.6', desc: '主力最强模型，追求效果' },\n   137→  balanced:    { main: 'g3f',     fast: 'g3f',  opus: 'opus4.6', desc: '性价比均衡' },\n   138→  economy:     { main: 'g3f',     fast: 'g3f',  opus: 'g3p',     desc: '最省 token' },\n   139→};\n   140→\n   141→// --- Helpers ---\n   142→\n   143→function findClaudeBin() {\n   144→  if (process.env.CLD_CLAUDE_BIN && fs.existsSync(process.env.CLD_CLAUDE_BIN)) {\n   145→    return process.env.CLD_CLAUDE_BIN;\n   146→  }\n   147→  const candidates = ['/opt/homebrew/bin/claude', '/usr/local/bin/claude'];\n   148→  for (const c of candidates) {\n   149→    try { if (fs.existsSync(c)) return c; } catch {}\n   150→  }\n   151→  return null;\n   152→}\n   153→\n   154→function readLastConfig() {\n   155→  const read = (name) => {\n   156→    try { return fs.readFileSync(path.join(CACHE_DIR, name), 'utf-8').trim(); }\n   157→    catch { return null; }\n   158→  };\n   159→  return {\n   160→    scenario: (read('last_scenario') || 'balanced').toLowerCase(),\n   161→    main: read('last_main') || '',\n   162→    fast: read('last_fast') || '',\n   163→    opus: read('last_opus') || '',\n   164→    autoUpgrade: read('last_auto_upgrade') === 'On',\n   165→    workDir: read('last_workdir') || '',\n   166→  };\n   167→}\n   168→\n   169→function writeLastConfig({ scenario, main, fast, opus, autoUpgrade, workDir }) {\n   170→  try {\n   171→    fs.mkdirSync(CACHE_DIR, { recursive: true });\n   172→    // Capitalize for cld interop (cld uses \"Balanced\", \"Custom\", etc.)\n   173→    fs.writeFileSync(path.join(CACHE_DIR, 'last_scenario'), scenario.charAt(0).toUpperCase() + scenario.slice(1));\n   174→    fs.writeFileSync(path.join(CACHE_DIR, 'last_main'), main);\n   175→    fs.writeFileSync(path.join(CACHE_DIR, 'last_fast'), fast);\n   176→    fs.writeFileSync(path.join(CACHE_DIR, 'last_opus'), opus);\n   177→    fs.writeFileSync(path.join(CACHE_DIR, 'last_auto_upgrade'), autoUpgrade ? 'On' : 'Off');\n   178→    if (workDir) fs.writeFileSync(path.join(CACHE_DIR, 'last_workdir'), workDir);\n   179→  } catch (err) {\n   180→    console.error('Failed to write last config:', err.message);\n   181→  }\n   182→}\n   183→\n   184→function resolveProxyBase() {\n   185→  // Priority: CLD_PROXY_BASE env > TOML main_port > default\n   186→  if (process.env.CLD_PROXY_BASE) return process.env.CLD_PROXY_BASE.replace(/\\/+$/, '');\n   187→  const port = parseMainPort();\n   188→  if (port) return `http://127.0.0.1:${port}`;\n   189→  return 'http://127.0.0.1:8145';\n   190→}\n   191→\n   192→// --- API Endpoints ---\n   193→\n   194→// API: Models, scenarios, and last config\n   195→app.get('/api/models', (req, res) => {\n   196→  const models = parseModelsFromToml();\n   197→  const lastConfig = readLastConfig();\n   198→  const autoUpgradeMap = parseAutoUpgradeMap();\n   199→\n   200→  // Build scenario list with resolved model info\n   201→  const scenarios = {};\n   202→  for (const [key, preset] of Object.entries(SCENARIO_PRESETS)) {\n   203→    scenarios[key] = { main: preset.main, fast: preset.fast, opus: preset.opus, desc: preset.desc };\n   204→  }\n   205→\n   206→  res.json({\n   207→    models,\n   208→    scenarios,\n   209→    autoUpgradeMap,\n   210→    lastConfig,\n   211→  });\n   212→});\n   213→\n   214→// API: List all sessions\n   215→app.get('/api/sessions', (req, res) => {\n   216→  const sessionList = Array.from(sessions.values()).map(s => ({\n   217→    id: s.id,\n   218→    model: s.model,\n   219→    scenario: s.scenario,\n   220→    cwd: s.cwd,\n   221→    created: s.created,\n   222→    active: s.pty ? true : false,\n   223→    history: s.history || ''\n   224→  }));\n   225→  res.json(sessionList);\n   226→});\n   227→\n   228→// API: Create new session\n   229→app.post('/api/sessions', express.json(), (req, res) => {\n   230→  // Check session limit\n   231→  if (sessions.size >= MAX_SESSIONS) {\n   232→    log(`Session limit reached (${MAX_SESSIONS})`);\n   233→    return res.status(400).json({ error: `会话数已达上限 (${MAX_SESSIONS})` });\n   234→  }\n   235→\n   236→  const {\n   237→    scenario = 'balanced',\n   238→    autoUpgrade = false,\n   239→    main: customMain,\n   240→    fast: customFast,\n   241→    opus: customOpus,\n   242→    workDir = '',\n   243→  } = req.body;\n   244→\n   245→  // Resolve working directory\n   246→  const cwd = workDir && fs.existsSync(workDir) ? workDir : env.HOME;\n   247→\n   248→  // Resolve models from scenario or custom\n   249→  let main, fast, opus;\n   250→  if (scenario === 'custom') {\n   251→    main = customMain || 'g3f';\n   252→    fast = customFast || 'g3f';\n   253→    opus = customOpus || 'opus4.6';\n   254→  } else {\n   255→    const preset = SCENARIO_PRESETS[scenario] || SCENARIO_PRESETS.balanced;\n   256→    main = preset.main;\n   257→    fast = preset.fast;\n   258→    opus = preset.opus;\n   259→  }\n   260→\n   261→  // Auto upgrade: check if ${main}.auto exists in routing\n   262→  if (autoUpgrade && main !== 'auto') {\n   263→    const autoModel = main + '.auto';\n   264→    const models = parseModelsFromToml();\n   265→    if (models.includes(autoModel)) {\n   266→      main = autoModel;\n   267→    }\n   268→  }\n   269→\n   270→  // Find claude binary\n   271→  const claudePath = findClaudeBin();\n   272→  if (!claudePath) {\n   273→    console.error('claude binary not found');\n   274→    return res.status(500).json({ error: 'claude binary not found' });\n   275→  }\n   276→\n   277→  // Build clean environment\n   278→  const env = {};\n   279→  env.TERM = 'xterm-256color';\n   280→  env.HOME = process.env.HOME || '/tmp';\n   281→  env.PATH = process.env.PATH || '';\n   282→  env.SHELL = process.env.SHELL || '/bin/zsh';\n   283→  env.USER = process.env.USER || '';\n   284→  env.LANG = process.env.LANG || 'en_US.UTF-8';\n   285→  env.XDG_CACHE_HOME = process.env.XDG_CACHE_HOME || path.join(env.HOME, '.cache');\n   286→\n   287→  // Model environment\n   288→  env.ANTHROPIC_MODEL = main;\n   289→  env.ANTHROPIC_DEFAULT_HAIKU_MODEL = fast;\n   290→  env.ANTHROPIC_DEFAULT_OPUS_MODEL = opus;\n   291→  env.CLAUDE_CODE_SUBAGENT_MODEL = fast;\n   292→  env.CLAUDE_CONFIG_DIR = CACHE_DIR;\n   293→\n   294→  // API key\n   295→  env.ANTHROPIC_API_KEY = process.env.CLD_PROXY_KEY || '';\n   296→\n   297→  // Base URL: only set if .claude.json exists (authenticated via cld)\n   298→  const claudeJson = path.join(CACHE_DIR, '.claude.json');\n   299→  if (process.env.CLD_USE_BASE_URL !== 'false' && fs.existsSync(claudeJson)) {\n   300→    env.ANTHROPIC_BASE_URL = resolveProxyBase();\n   301→  }\n   302→\n   303→  // Clear proxy env to prevent pollution\n   304→  env.HTTP_PROXY = '';\n   305→  env.HTTPS_PROXY = '';\n   306→  env.ALL_PROXY = '';\n   307→  env.ANTHROPIC_AUTH_TOKEN = '';\n   308→\n   309→  const sessionId = uuidv4();\n   310→\n   311→  let ptyProcess;\n   312→  try {\n   313→    log(`Spawning claude: ${claudePath}, model=${main}, fast=${fast}, opus=${opus}, scenario=${scenario}, cwd=${cwd}`);\n   314→    ptyProcess = pty.spawn(claudePath, [], {\n   315→      name: 'xterm-256color',\n   316→      cols: 80,\n   317→      rows: 30,\n   318→      cwd: cwd,\n   319→      env: env\n   320→    });\n   321→    log('PTY spawned successfully, pid:', ptyProcess.pid);\n   322→  } catch (err) {\n   323→    console.error('Failed to spawn claude:', err);\n   324→    return res.status(500).json({ error: 'Failed to start claude process: ' + err.message });\n   325→  }\n   326→\n   327→  const session = {\n   328→    id: sessionId,\n   329→    model: main,\n   330→    scenario,\n   331→    cwd: cwd,\n   332→    created: new Date().toISOString(),\n   333→    pty: ptyProcess,\n   334→    ws: null,\n   335→    history: []  // Terminal output history\n   336→  };\n   337→\n   338→  sessions.set(sessionId, session);\n   339→\n   340→  // Write last config for cld interop\n   341→  writeLastConfig({ scenario, main, fast, opus, autoUpgrade, workDir: req.body.workDir });\n   342→\n   343→  // Handle process output\n   344→  ptyProcess.onData((data) => {\n   345→    // Store in history (limit to last 10000 chars